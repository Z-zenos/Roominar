{
  "name": "Roominar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-api": "npx @openapitools/openapi-generator-cli generate --enable-post-process-file -g typescript -t generator/template/typescript -o src/lib/api/generated --additional-properties=supportsES6=true,useObjectParameters=true -i",
    "generate-api-local": "npm run generate-api -- http://localhost:8000/api/v1/openapi.json && eslint --fix src/lib/api/generated || echo 0",
    "generate-api-deploy": "npm run generate-api -- src/lib/api/apidoc.json && eslint --fix src/lib/api/generated || echo 0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@tanstack/react-query": "^5.50.1",
    "dayjs": "^1.11.11",
    "jwt-decode": "^4.0.0",
    "next": "14.1.0",
    "next-auth": "^4.24.7",
    "next-intl": "^3.17.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.1",
    "react-icons": "^5.2.1",
    "recoil": "^0.7.7",
    "recoil-persist": "^5.1.0",
    "sharp": "^0.33.4",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.4",
    "typescript": "5.3.3"
  }
}